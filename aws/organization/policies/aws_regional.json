{
    "Version": "2012-10-17",
    "Statement": [{
        "Sid": "DenyUnapprovedRegions",
        "Effect": "Deny",
        "NotAction": [
            "iam:*",
            "organizations:*",
            "route53:*",
            "budgets:*",
            "waf:*",
            "cloudfront:*",
            "globalaccelerator:*",
            "importexport:*",
            "support:*"
        ],
        "Resource": "*",
        "Condition": {
            "StringNotEquals": {
                "aws:RequestedRegion": [
                    "us-east-1",
                    "us-west-2"
                ]
            }
        }
    }]
}
